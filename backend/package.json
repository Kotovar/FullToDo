{
  "name": "fulltodo_backend",
  "version": "0.0.0",
  "main": "src/app.ts",
  "scripts": {
    "build": "tsc",
    "start": "node -r tsconfig-paths/register dist/app.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/app.ts",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "prepare": "cd .. && husky backend/.husky",
    "lint-staged": "lint-staged",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "Kotovar",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "morgan": "^1.10.0",
    "shared": "file:../shared",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/morgan": "^1.9.9",
    "@types/supertest": "^6.0.3",
    "nodemon": "^3.1.9",
    "supertest": "^7.1.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  }
}
